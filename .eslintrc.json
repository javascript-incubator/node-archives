{
  "extends": ["airbnb-base", "prettier"],
  "parser": "babel-eslint",
  "plugins": ["prettier", "import", "flowtype", "flowtype-errors"],
  "rules": {
    "prettier/prettier": "error",
    "func-names": ["error", "never"],
    "no-param-reassign": 0,
    "class-methods-use-this": 0,
    "import/no-extraneous-dependencies": 0,
    "prefer-template": 0,
    "no-console": 0,
    "global-require": 0
  },
  "globals": {
    "Transformer": true,
    "Types": true
  }
}
